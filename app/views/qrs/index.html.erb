<p id="notice"><%= notice %></p>

<h1>Qrs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qrs.each do |qr| %>
      <tr>
        <td><%= qr.user_id %></td>
        <td><%= link_to 'Show', qr %></td>
        <td><%= link_to 'Edit', edit_qr_path(qr) %></td>
        <td><%= link_to 'Destroy', qr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qr', new_qr_path %>
